<%- include('../include/head.ejs') %>
<link rel="stylesheet" href="/css/style.css" media="screen" title="no title" charset="utf-8">
<script src="https://code.jquery.com/jquery-2.2.4.js" charset="utf-8"></script>
<meta name="robots" content="noindex,follow" />
</head>
<%- include('../include/middle.ejs') %>
<div class="shopping-cart">
    <div class="title">
        Shopping Bag
    </div>
    <div class="title">
        TotalPrice : INR. <%=totalpr%>
    </div>

    <%if( passing.length > 0){%>
        <% for(let pr of passing){%>
            <div class="item">
                <div class="image">
                    <img src='<%=pr.productId.path%>' width=90px height=90px alt="not-loading" />
                </div>

                <div class="description">
                    <span><%=pr.productId.title%></span>
                    <span>White</span>
                </div>

                <div class="quantity">
                    <form method='POST' action='/addtocart2'>
                        <button class="plus-btn" type='submit' >
                            <img src="/public/plus.svg" alt="" />
                        </button>
                        <input type='hidden' name='detailid' value='<%=pr.productId._id%>'>
                    </form>

                    <input type="text" name="name" value="<%=pr.qty%>">

                    <form method='POST' action='/deletefromcart'>
                        <button class="minus-btn" type='submit' >
                            <img src="/public/minus.svg" alt="" />
                        </button> 
                        <input type='hidden' name='idd' value='<%=pr.productId._id%>'>
                        <input type='hidden' name='prr' value='<%=pr.productId.price%>'>
                    </form>
                </div>

                <div class="total-price">INR. <%=pr.productId.price%></div>
            </div>
            
        <%}%>
        <a href='/orders' >
            <img width=150px height=70px src="data:image/jpeg;base64," alt="hidden web image">
        </a>
    <%}else{%>
        <div class="title">
            Firstly, add some items.
        </div>
    <%}%>
</div>
</body>
</html>